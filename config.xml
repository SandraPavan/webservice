<?xml version='1.0' encoding='utf-8'?>
<widget id="br.finch.lp5.pedidos" version="0.0.1" xmlns="http://www.w3.org/ns/wi" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Previsão do Tempo</name>
    <description> App que apresenta Previsão do Tempo </description>
    <author email="sandrapavan@gmail.com" href="http://fibr.br">

 Sandra Pavan

</author>
    <preference name="Orientation" value="portrait" />
    <preference name="Fullscreen" value="false" />
    <preference name="BackgroundColor" value="0xeceff1FF" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="SplashScreen" value="screen" />
    <preference name="SplashScreenDelay" value="1500" />
    <preference name="SplashShowOnlyFirstTime" value="false" />
    <engine name="android" />
    <plugin name="cordova-plugin-whitelist" spec="~1.3.0" />
    <access origin="*" />
</widget>
